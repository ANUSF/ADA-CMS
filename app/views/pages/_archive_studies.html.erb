  <aside class="datasets">
    <h2>New datasets</h2>
    <h3>Recent data deposits <br>
      <%=
	if last = @archive_studies.last
	  '(since ' + last.created_at.strftime('%B %Y') + ')'
	else
	  'No recent data'
	end
      %>
    </h3>
    <ul>
      <%= render :partial => "archive_study", :collection => @archive_studies %>
    </ul>
  </aside>
