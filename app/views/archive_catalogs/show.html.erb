<% content_for(:js) do %>
  $(document).ready(function() {    
    $('#browse-results').html("Please select an archive from the left");
  });
<% end %>

<% content_for(:sidenav) do
    %>
      <%= render :partial => 'catalog_side_nav' %>
<% end %>

<% content_for(:breadcrumbs) do %>
  <% if @catalog %>
	  Browsing <%= @catalog.path.slug.gsub("/browse", "")  %>
	<% else %>
	  Browsing <%= @current_archive.name %>
	<% end %>
<% end %>

<div id="browse-results">
</div>