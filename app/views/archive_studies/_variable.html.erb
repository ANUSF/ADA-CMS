<% fields = VariableField.to_hash(variable) %>

<li id="<%= variable.study.ddi_id %>-<%= variable.id %>">
	Label: <%= variable.label %> <br/>
	Name: <%= variable.name %> <br/>
	Numcats: <%= variable.num_cats %> <br/>
	Val Range Max: <%= variable.val_range_max %> <br/>
	Val Range Max: <%= variable.val_range_min %> <br/>
	
	<% for field in fields %>
	  <%= variable_field(field) %> <br/>
	<% end %>	
</li>